{
  "name": "banner-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "dev-gen": "concurrently \"nodemon dist/index.js\" \"yarn generate --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "yarn generate",
    "clean:database": "npx ts-node scripts/CleanDatabase.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "migrate:create": "npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "drizzle:migration:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "drizzle:migration:push": "drizzle-kit push:pg",
    "drizzle:migration:deploy": "npx ts-node scripts/RunMigration.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moellejc/banner-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moellejc/banner-server/issues"
  },
  "homepage": "https://github.com/moellejc/banner-server#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/express": "^4.17.9",
    "@types/geojson": "^7946.0.7",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.16",
    "@types/pg": "^8.10.9",
    "concurrently": "^5.3.0",
    "drizzle-kit": "^0.20.8",
    "nodemon": "^2.0.6",
    "prisma": "^4.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@prisma/client": "^4.2.1",
    "@types/google.maps": "^3.55.7",
    "apollo-server-express": "^2.19.1",
    "argon2": "^0.27.1",
    "axios": "^1.2.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^8.0.3",
    "dotenv-safe": "^8.2.0",
    "drizzle-orm": "^0.29.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "geojson": "^0.5.0",
    "graphql": "^15.4.0",
    "h3-js": "^4.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "oauth-1.0a": "^2.2.6",
    "pg": "^8.5.1",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
